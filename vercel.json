{
    "builds": [
      {
        "src": "health_care/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",  
          "runtime": "python3.13"
        }
      }
    ],
    "functions": {
      "*": {
        "memory": 2048,        
        "maxDuration": 30      
      }
    },
    "routes": [
      {"src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "health_care/wsgi.py"      
        
      }
    ]
  }
  